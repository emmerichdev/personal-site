{
  "name": "personal-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "pages:dev": "wrangler pages dev --compatibility-date=2024-01-01 -- vite dev",
    "pages:deploy": "npm run build && wrangler pages deploy ./dist",
    "db:init": "wrangler d1 execute personal-blog --local --file=./schema.sql",
    "db:init:remote": "wrangler d1 execute personal-blog --remote --file=./schema.sql"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@tanstack/query-async-storage-persister": "^5.90.13",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-persist-client": "^5.90.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "wrangler": "^4.50.0"
  }
}
